{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "prisma/schema.prisma",
            "use": "@vercel/prisma",
            "config": {
                "prismaClientPath": "./node_modules/.prisma/client"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.ts"
        }
    ],
    "env": {
        "DATABASE_URL": "@database_url"
    }
}